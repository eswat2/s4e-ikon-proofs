{
  "name": "svelte-app",
  "version": "0.0.6",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "echo skipping...",
    "prebuild": "yarn format",
    "predeploy": "yarn build; yarn version --patch",
    "deploy": "cd dist; ln -s ../.now .vercel; vc --prod",
