    "open": "open https://s4e-ikon-proofs.vercel.app"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "1.0.0-next.30",
    "prettier": "2.4.1",
    "rangetouch": "2.0.1",
    "svelte": "3.44.1",
    "vite": "2.6.13"
  }
}
