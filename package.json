{
  "name": "svelte-app",
  "version": "0.0.1253",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "bump": "bumpp patch --commit --tag --push --yes",
    "deploy-app": "cd dist; ln -s ../.vercel; vc --prod",
    "release": "pnpm bump && pnpm build && pnpm deploy-app",
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "echo skipping...",
    "prebuild": "echo skipping pnpm format",
    "spell": "cspell './src/**/*.{css,js,ts,svelte}'",
    "cc": "prettier --check './src/**/*.{css,js,ts,svelte}'",
    "format": "prettier --write './src/**/*.{css,js,ts,svelte}'",
    "open": "open https://s4e-ikon-proofs.vercel.app"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "6.2.4",
    "bumpp": "10.4.1",
    "cspell": "9.6.4",
    "only-allow": "1.2.2",
    "prettier": "3.8.1",
    "prettier-plugin-svelte": "3.4.1",
    "rangetouch": "2.0.1",
    "svelte": "5.50.2",
    "vite": "7.3.1",
    "yoctocolors": "2.1.2"
  }
}
