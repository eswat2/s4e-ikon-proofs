  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "1.0.0-next.42",
    "cspell": "5.20.0",
    "prettier": "2.6.2",
    "prettier-plugin-svelte": "2.7.0",
    "rangetouch": "2.0.1",
    "svelte": "3.48.0",
    "vite": "2.9.7"
  }
}
