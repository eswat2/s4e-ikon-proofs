{
  "name": "svelte-app",
  "version": "0.0.480",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "echo skipping...",
    "prebuild": "yarn format",
    "predeploy": "yarn build; yarn version --patch",
    "deploy": "cd dist; ln -s ../.now .vercel; vc --prod",
    "spell": "cspell *.{html,js,md} src/*/*.{css,js,svelte}",
    "cc": "prettier --check src/*/*.{css,js,svelte}",
    "format": "prettier --write cSpell.json index.html src/*/*.{css,js,svelte}",
    "open": "open https://s4e-ikon-proofs.vercel.app"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "2.4.2",
    "cspell": "6.31.2",
    "prettier": "3.0.0",
    "prettier-plugin-svelte": "3.0.0",
    "rangetouch": "2.0.1",
    "svelte": "4.1.0",
    "vite": "4.4.4"
  }
}
